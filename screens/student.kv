#: import webbrowser  webbrowser
#: import Window kivy.core.window.Window

<BaseShadowWidget>:
    size_hint: None, None
    size: 100, 100
    md_bg_color: 0, 0, 1, 1
    elevation: 36
    pos_hint: {'center_x': .5}	
    
<IconRound@MyMDCard>:
	source:"gradients/"+app.gra
	icon:""
	text:""
	radius:"20dp"
	style:"elevated"
	size_hint:None,None
	size:dp(150),dp(120)
	ripple_behavior:True
	md_bg_color:app.theme_cls.accent_light
	RelativeLayout:
		FitImage:
			source:root.source
			radius:"20dp"
		AnchorLayout:
			MDIconButton:
				icon:root.icon
				on_press:root.on_press
		MDLabel:
			halign:"center"
			pos_hint:{"center_x":0.5,"center_y":0.20}
			text:root.text
			font_name:"assets/Poppins-Bold.ttf"
			font_size:"15sp"


<MyLayout@MDSwiperItem>:
	img:""
	FitImage:
        source: root.img
        radius: [20,]					
<DataloaderLabel@AKLabelLoader>
    size_hint_y: None
    height: dp(20)
    theme_text_color: "Primary"
    halign: "left"
				
<HeadItem>:
	size_hint:None,None
	size:app.y-dp(20),dp(120)
	avatar:""
	title:''
	content:""
	MyMDCard
		size_hint:None,None
		size:app.y-dp(20),dp(120)
		radius:dp(20)
		md_bg_color:app.theme_cls.bg_light
		MDBoxLayout:
			MDFloatLayout:
				size_hint_x: .3
				AKImageLoader:
					size_hint: None,None
					size: dp(50), dp(50)
					pos_hint: {"center_x": .5 , "center_y": .5}
					source: root.avatar
			MDBoxLayout:
				orientation: "vertical"
				padding:dp(32)
				spacing:dp(15)
				DataloaderLabel:
					text:  root.title
					font_name:"assets/Poppins-Bold.ttf"
					font_size:"15sp"

				MDSeparator:

				DataloaderLabel:
					text:  root.content
					font_name:"assets/Poppins-Regular.ttf"
					font_size:"10sp"

<FitImgCard@AnchorLayout>
	text:""
	img:""
	link:""
	size_hint:None,None
	size: app.y//2, "140dp"
	MyMDCard:
		radius:"20dp"
		elevation:dp(30)
		halign:"center"
		size_hint:None,None
		style:"elevated"
		size:app.y//2, "140dp"
		md_bg_color:app.theme_cls.accent_light
