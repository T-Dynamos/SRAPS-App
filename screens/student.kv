			id:ooo
			FitImage:
				source:root.img
				radius:"20dp"
			MyMDCard:
				pos_hint:{"center_x":0.5,"center_y":0.03}
				size_hint:None,None
				size:app.y//2, "40dp"
				on_press: webbrowser.open(root.link)
				md_bg_color:app.theme_cls.primary_light
				radius:0,0,"20dp",'20dp'
				style:"elevated"
				elevation:dp(30)
				RelativeLayout:
					MDLabel:
						halign:"center"
						text:root.text
						font_name:"assets/Poppins-Bold.ttf"
						font_size:"15sp"
						
<FitCard2@MyMDCard>
	text:""
	image:""
